{
    "name": "dldash/data-transfer-object",
    "description": "Data transfer objects.",
    "license": "MIT",
    "require": {
        "php": "^8.0",
        "ext-json": "*"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.0"
    },
    "autoload": {
        "psr-4": {
            "Dldash\\DataTransferObject\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dldash\\DataTransferObject\\Tests\\": "tests"
        }
    },
    "scripts": {
        "test": "vendor/bin/phpunit",
        "test-coverage": "vendor/bin/phpunit --coverage-html coverage"
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true
    },
    "minimum-stability": "dev"
}
